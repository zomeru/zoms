{
  "name": "zoms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run test-all && next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "postbuild": "next-sitemap",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint .",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types",
    "test-all:build": " npm run test-all && npm run build",
    "prepare": "husky",
    "studio:dev": "cd studio && pnpm run dev",
    "studio:build": "cd studio && pnpm run build",
    "studio:deploy": "cd studio && pnpm run deploy"
  },
  "dependencies": {
    "@google/genai": "^1.36.0",
    "@portabletext/react": "^6.0.2",
    "@portabletext/types": "^4.0.1",
    "@rehype-pretty/transformers": "^0.13.2",
    "@sanity/client": "^7.14.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "dompurify": "^3.3.1",
    "jsdom": "^27.4.0",
    "next": "16.1.2",
    "next-sanity": "^12.0.12",
    "next-sitemap": "^4.2.3",
    "react": "19.2.4",
    "react-dom": "19.2.3",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-modal": "^3.16.3",
    "rehype-external-links": "^3.0.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/jsdom": "^27.0.0",
    "@types/node": "25.0.8",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@types/react-modal": "^3.16.3",
    "autoprefixer": "^10.4.23",
    "eslint": "9.39.2",
    "eslint-config-love": "^145.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "lint-staged": {
    "*{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.18.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "@vercel/speed-insights",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "engines": {
    "node": "24.x"
  }
}
